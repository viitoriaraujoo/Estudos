   "source": [
    "# Uso do subplot na criação de múltiplos gráficos"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "id": "ba3c970a",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 1200x600 with 2 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "# Criação do local, configuração do plot\n",
    "\n",
    "fig,axes = plt.subplots(\n",
    "    1, #Número de linhas\n",
    "    2, #Número de colunas\n",
    "    figsize=(12, 6) #Tamanho do Plot\n",
    ")\n",
    "\n",
    "# Gráfico de Dispersão de BMI x Charges na posição 0 \n",
    "# (Linha 0, Coluna 0)\n",
    "axes[0].scatter(insurance['bmi'], insurance['charges'])\n",
    "axes[0].set_xlabel('BMI') # Define o rótulo do eixo x do primeiro subplot\n",
    "axes[0].set_ylabel('charges') #Define o rótulo do eixo y do primeiro subplot\n",
    "axes[0].set_title('Charges vs BMI')\n",
    "\n",
    "# Gráfico de Dispersão de BMI x Charges na posição 1 \n",
    "# (Linha 0, Coluna 1)\n",
    "axes[1].scatter(insurance['age'], insurance['charges'])\n",
    "axes[1].set_xlabel('age')\n",
    "axes[1].set_ylabel('charges')\n",
    "axes[1].set_title('Charges vs Age')\n",
    "\n",
    "# Ajuste de espaço entre os plots\n",
    "plt.tight_layout # Ajusta automaticamente o espaçamento entre subplots para que eles não se sobreponham.\n",
    "\n",
    "# Apresenta o gráfico\n",
    "plt.show()"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "64990ec6",
   "metadata": {},
   "source": [
    "# Análise envolvendo variável categórica e uma numérica"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "id": "45b526fc",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 1200x800 with 4 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "\n",
    "# Calcular as médias das cobranças para cada categoria de 'sex'\n",
    "mean_charges_sex = insurance.groupby('sex')['charges'].mean()\n",
    "\n",
    "# Calcular as médias das cobranças para cada categoria de 'region'\n",
    "mean_charges_region = insurance.groupby('region')['charges'].mean()\n",
    "\n",
    "# Calcular as médias das cobranças para cada categoria de 'children'\n",
    "mean_charges_children = insurance.groupby('children')['charges'].mean()\n",
    "\n",
    "# Calcular as médias das cobranças para cada categoria de 'smoker'\n",
    "mean_charges_smoker = insurance.groupby('smoker')['charges'].mean()\n",
    "\n",
    "# Cores Personalizadas para as barras\n",
    "colors = ['#2b8cbe', '#fe9929', '#31a354', '#a1d99b']\n",
    "\n",
    "# Criar uma figura com subplots\n",
    "fig, axes = plt.subplots(2, 2, figsize=(12, 8))\n",
    "\n",
    "# Gráfico de barras: média das cobranças por sexo\n",
    "mean_charges_sex.plot(kind='bar', ax=axes[0, 0], color=colors)\n",
    "axes[0, 0].set_xlabel('Sexo')\n",
    "axes[0, 0].set_ylabel('Cobranças Médias')\n",
    "axes[0, 0].set_title('Cobranças Médias por Sexo')\n",
    "\n",
    "# Gráfico de barras: média das cobranças por região\n",
    "mean_charges_region.plot(kind='bar', ax=axes[0, 1], color=colors)\n",
    "axes[0, 1].set_xlabel('Região')\n",
    "axes[0, 1].set_ylabel('Cobranças Médias')\n",
    "axes[0, 1].set_title('Cobranças Médias por Região')\n",
    "\n",
    "# Gráfico de barras: média das cobranças por filhos\n",
    "mean_charges_children.plot(kind='bar', ax=axes[1, 1], color=colors)\n",
    "axes[1, 1].set_xlabel('Número de Filhos')\n",
    "axes[1, 1].set_ylabel('Cobranças Médias')\n",
    "axes[1, 1].set_title('Cobranças Médias por Número de Filhos')\n",
    "\n",
    "# Gráfico de barras: média das cobranças por fumante\n",
    "mean_charges_smoker.plot(kind='bar', ax=axes[1, 0], color=colors)\n",
    "axes[1, 0].set_xlabel('Fumante')\n",
    "axes[1, 0].set_ylabel('Cobranças Médias')\n",
    "axes[1, 0].set_title('Cobranças Médias por Fumante')\n",
    "\n",
    "# Ajustar espaçamento entre os subplot\n",
    "plt.tight_layout()\n",
    "\n",
    "# Exibir o Gráfico\n",
    "plt.show()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "id": "e1671e10",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 1200x1000 with 4 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "import seaborn as sns\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Criar uma figura com 2 linhas e 2 colunas\n",
    "fig, ax= plt.subplots (2,2, figsize=(12,10))\n",
    "\n",
    "# Primeira linha, primeira coluna.\n",
    "sns.countplot(data=insurance, x='sex', ax=ax[0,0])\n",
    "ax[0,0].set_title('Sex')\n",
    "\n",
    "# Primeira linha, segunda coluna.\n",
    "sns.countplot(data=insurance, x='region', ax=ax[0,1])\n",
    "ax[0,1].set_title('Region')\n",
    "\n",
    "# Segunda linha, primeira coluna.\n",
    "sns.countplot(data=insurance, x='smoker', ax=ax[1,0])\n",
    "ax[1,0].set_title('Smoker')\n",
    "\n",
    "# Segunda linha, segunda coluna.\n",
    "sns.countplot(data=insurance, x='children', ax=ax[1,1])\n",
    "ax[1,1].set_title('Children')\n",
    "\n",
    "# Remover spines do topo e da direita\n",
    "for a in ax.flat:\n",
    "    a.spines['top'].set_visible(False)\n",
    "    a.spines['right'].set_visible(False)\n",
    "\n",
    "# Ajusta o layout para evitar sobreposição\n",
    "plt.tight_layout()\n",
    "\n",
    "# Mostra os Gráficos\n",
    "plt.show()"
   ]
  },
