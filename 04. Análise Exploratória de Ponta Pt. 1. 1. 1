    "import matplotlib.pyplot as plt #Biblioteca para visualização de gráficos\n",
    "\n",
    "# Cria uma contagem para cada tipo de região\n",
    "region_types = insurance['region'].value_counts() # Contagem de cada valor único\n",
    "\n",
    "# Listamos as regiões existentes\n",
    "region_name = ['southwest', 'southeast', 'northwest', 'northeast']\n",
    "\n",
    "# Personaliza as cores para cada barra\n",
    "colors = ['steelblue', 'lightcoral', 'mediumseagreen', 'orange']\n",
    "\n",
    "# Cria uma Figura e um eixo com largura de 8 polegadas e 5 de altura\n",
    "fig, ax = plt.subplots(figsize=(8, 6)) \n",
    "\n",
    "# Plota o gráfico de barras com as cores personalizadas\n",
    "ax.bar(region_name, region_types.values, color=colors)\n",
    "\n",
    "# Configuração dos rótulos dos eixos e títulos\n",
    "ax.set_xlabel('Região')\n",
    "ax.set_ylabel('Contagem')\n",
    "ax.set_title('Contagem de Regiões')\n",
    "\n",
    "# Adicionar a contagem sobre cada barra\n",
    "for i, v in enumerate(region_types.values): #posição no eixo x e y\n",
    "    ax.text(i, v, str(v), ha='center', va='bottom') \n",
    "\n",
    "# O loop facilita o raciocinio, pois senão, teriamos que realizar manualmente com ax.text\n",
    "    \n",
    "    \n",
    "# Exibe o gráfico\n",
    "plt.show()\n"
   ]
  },
