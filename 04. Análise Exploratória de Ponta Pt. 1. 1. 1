    "# Criação do Gráfico de Dispersão\n",
    "\n",
    "fig,ax= plt.subplots(figsize=(8,6)) \n",
    "ax.scatter(insurance['age'], insurance['charges'], c=\"red\")\n",
    "\n",
    "# Configuração do título e rótulos dos eixos\n",
    "ax.set_xlabel('Idade', fontsize=8)\n",
    "ax.set_ylabel('Cobranças', fontsize=8)\n",
    "ax.set_title('Gráfico de Dispersão Idade vs Cobrança', fontsize=14)\n",
    "\n",
    "# Remove as linhas do topo e da direita\n",
    "ax.spines['top'].set_visible(False) \n",
    "ax.spines['right'].set_visible(False) \n",
    "\n",
    "#Alterar o estilo dos gráficos \n",
    "plt.style.use (\"fivethirtyeight\")\n",
    "\n",
    "plt.show()"
   ]
