   "source": [
    "# Calcular a média da cobrança por sexo\n",
    "mean_charges_sex = insurance.groupby('sex')['charges'].mean()\n",
    "\n",
    "# Personalização das cores para cada barra\n",
    "colors = ['blue', 'pink']\n",
    "\n",
    "# Cria uma Figura e um Eixo\n",
    "fig, ax = plt.subplots(figsize=(8, 6))  # Corrigi o erro aqui, adicionando o sinal de igual\n",
    "\n",
    "# Plota o gráfico de barras com as cores personalizadas\n",
    "ax.bar(mean_charges_sex.index, mean_charges_sex.values, color=colors)\n",
    "\n",
    "# Configuração dos rótulos dos eixos e título\n",
    "ax.set_xlabel('Sexo')  # Corrigi o nome do método para definir o rótulo do eixo x\n",
    "ax.set_ylabel('Cobranças')  # Corrigi o nome do método para definir o rótulo do eixo y\n",
    "ax.set_title('Média das Cobranças por Sexo')\n",
    "\n",
    "# Adiciona manualmente os textos dos rótulos\n",
    "for i, value in enumerate(mean_charges_sex.values):\n",
    "    ax.text(i, value, f'{value:.2f}', ha='center', va='bottom', fontsize=12)\n",
    "    \n",
    "# Define limites dos eixos x e y\n",
    "ax.set_xlim(-0.5, 1.5)\n",
    "ax.set_ylim(0, 16000)\n",
    "\n",
    "# Calcula a diferença percentual entre os dois sexos\n",
    "diff_percentual = ((mean_charges_sex['male'] - mean_charges_sex['female']) / mean_charges_sex['female']) * 100\n",
    "\n",
    "# Mensagem sobre a diferença salarial\n",
    "ax.text(\n",
    "    1.2, #posição no eixo x\n",
    "    mean_charges_sex.values[1]+1000, #posição no eixo y\n",
    "    f'0 salário dos homens é {diff_percentual:.2f}% maior' #mensagem\n",
    ")\n",
    "\n",
    "# Exibe o gráfico\n",
    "plt.show()\n"
   ]
