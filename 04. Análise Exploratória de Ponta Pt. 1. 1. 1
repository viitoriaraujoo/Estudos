   "source": [
    "# Cria uma contagem\n",
    "smoke_counts= insurance['smoker'].value_counts()\n",
    "\n",
    "colors= ['steelblue', 'lightcoral'] # Cores das barras\n",
    "\n",
    "# Atribuindo o objeto da figura à variável fig e o objeto do eixo à variável ax.\n",
    "fig,ax= plt.subplots(figsize=(8,6)) # Fig é a área em branco onde o gráfico será desenhado, ax é o eixo em que o gráfico é plotado.\n",
    "# Cria gráfico de barras com indice e contagem\n",
    "ax.bar(\n",
    "    smoke_counts.index, # Valor do eixo x\n",
    "    smoke_counts.values, # Valor do eixo y\n",
    "    color= colors # Cores das barras\n",
    ")\n",
    "\n",
    "# Rótulo do Eixo X com fonte de tamanho 8\n",
    "ax.set_xlabel('Smoker', fontsize=8)\n",
    "\n",
    "# Rótulo do Eixo Y com fonte de tamanho 8\n",
    "ax.set_ylabel('Count', fontsize=8)\n",
    "\n",
    "# Título do Gráfico com fonte de tamanho 14\n",
    "ax.set_title('Distribuição de Fumantes', fontsize=14)\n",
    "\n",
    "# Remove as linhas do topo e da direita\n",
    "ax.spines['top'].set_visible(False) \n",
    "ax.spines['right'].set_visible(False) \n",
    "\n",
    "plt.show()"
   ]
